    "outDir": "dist",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES5",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["*.ts", "*.tsx"]
}